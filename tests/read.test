#\newline
#\vtab
#\page
#\return
#\space
(display "\"")
(display "\\")
(display "\\\"\"\\")
(display 'foo)(newline)
(display '(foo))(newline)
(display '('foo))(newline)
(display (cdr '(car . cdr))) (newline)
(display "foo bar") (newline)
